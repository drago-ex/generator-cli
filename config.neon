# nette extensions
extensions:
	dibi: Dibi\Bridges\Nette\DibiExtension22
	console: Contributte\Console\DI\ConsoleExtension(%consoleMode%)
	generator: Drago\Generator\DI\GeneratorExtension


# generator options
generator:
	path: entity
	namespace: Entity


# symfony console
console:
	name: Symfony Console
	lazy: false


# database connection
dibi:
	host: 127.0.0.1
	username:
	password:
	database:
	driver: mysqli
	lazy: true
